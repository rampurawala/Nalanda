    <style name="AppThemeSplash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimarybg</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkbg</item>
        <item name="colorAccent">@color/textbg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <!--<item name="android:windowDisablePreview">true</item>-->
        <!--android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"-->
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

